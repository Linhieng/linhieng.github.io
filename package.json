        "@sveltejs/kit": "^1.25.0",
        "@typescript-eslint/eslint-plugin": "^6.7.2",
        "@typescript-eslint/parser": "^6.7.2",
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-svelte": "^2.33.2",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.1",
        "markdownlint-cli": "^0.36.0",
        "prettier": "^3.0.3",
        "prettier-plugin-svelte": "^3.0.3",
        "svelte": "^4.2.1",
